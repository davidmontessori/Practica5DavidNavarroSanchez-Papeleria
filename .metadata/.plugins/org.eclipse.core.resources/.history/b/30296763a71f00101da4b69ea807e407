package clases;

public class Lapiz2 extends Producto{
	
	private String tipoMina;
	private String dureza;
	
	public Lapiz2 Producto(String nombre, double precio, String marca, Proveedores proveedor, String tipoMina, String dureza) {
		super(nombre, precio, marca, proveedor);
		this.dureza=dureza;
		this.tipoMina=tipoMina;
		
	}

}
